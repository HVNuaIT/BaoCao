package entity;



public class Cart {
 private int id;
 private Product product;
 private int soluong;
 private long gia ;
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public Product getProduct() {
	return product;
}
public void setProduct(Product product) {
	this.product = product;
}
public int getSoluong() {
	return soluong;
}
public void setSoluong(int soluong) {
	this.soluong = soluong;
}
public long getGia() {
	return gia;
}
public void setGia(long gia) {
	this.gia = gia;
}
public Cart() {
	
}
public Cart(int id, Product product, int soluong, long gia) {
	super();
	this.id = id;
	this.product = product;
	this.soluong = soluong;
	this.gia = gia;
} 
 
}
